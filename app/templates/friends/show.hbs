
<div class="friend-profile">

<h2>{{model.fullName}}</h2>

<p>
  {{model.firstName}}
</p>
<p>
  {{model.lastName}}
</p>
<p>
  {{model.email}}
</p>
<p>
  {{model.twitter}}
</p>

<p>
  {{#link-to 'friends.edit' model}}
    edit
  {{/link-to}}
</p>

<p>
  <a href="#" {{action "delete" model}}>delete</a>
</p>
</div>
